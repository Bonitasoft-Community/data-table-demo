    "labelProvider" : {
      "type" : "expression",
      "value" : [
        "return {",
        "     userName: function(user){",
        "        return !user.id ? 'Unassigned' : user.id == '-1' ? 'Any' :  user.firstname + ' ' + user.lastname;",
        "     }",
        "}"
      ],
      "exposed" : false
    },
    "initDataProcess" : {
      "type" : "url",
      "value" : [
        "../API/bpm/process?p=0&c=1&f=name=Initialize Data"
      ],
      "advancedOptions" : {
        "headers" : "",
        "statusCode" : "",
        "failedResponseValue" : ""
      },
      "exposed" : false
    },
    "dataProvider" : {
      "type" : "expression",
      "value" : [
        "let assigneeArray = []",
        "let reporterArray = []",
        "if($data.users){",
        "    const any = {userName:'Any', id:'-1'};",
        "    const unassigned = {userName:'Unassigned'};",
        "    assigneeArray = [...$data.users];",
        "    assigneeArray.unshift(any, unassigned);",
        "    ",
        "    reporterArray = [...$data.users];",
        "    reporterArray.unshift(any);",
        "}",
        "return {",
        "    reporters : reporterArray, ",
        "    assignees : assigneeArray",
        "}"
      ],
      "exposed" : false
    }
